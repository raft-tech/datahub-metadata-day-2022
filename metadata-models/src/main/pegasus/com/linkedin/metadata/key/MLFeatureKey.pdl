namespace com.linkedin.metadata.key

/**
 * Key for an MLFeature
 */
@Aspect = {
  "name": "mlFeatureKey"
}
record MLFeatureKey {

  /**
   * Namespace for the feature
   */
  @Searchable = {
    "fieldType": "TEXT_PARTIAL",
  }
  featureNamespace: string

  /**
   * Name of the feature
   */
  @Searchable = {
    "fieldType": "TEXT_PARTIAL",
    "enableAutocomplete": true,
    "boostScore": 8.0
  }
  name: string
}