namespace com.linkedin.assertion

/**
 * Single parameter for AssertionStdOperators.
 */
record AssertionStdParameter {
  /**
   * The parameter value
   */
  value: string

  /**
   * The type of the parameter
   */
  type: enum AssertionStdParameterType {
    STRING
    NUMBER
    LIST
    SET
    UNKNOWN
  }
}