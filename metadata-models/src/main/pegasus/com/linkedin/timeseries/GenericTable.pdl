namespace com.linkedin.timeseries

/**
 * Defines a generic table.
 */
record GenericTable{
  /**
   * The names of the columns.
   */
  columnNames: array[string]

  /**
   * The types of the columns.
   */
  columnTypes: array[string]

  /**
   * The data rows.
   */
  rows: optional array[array[string]]
}