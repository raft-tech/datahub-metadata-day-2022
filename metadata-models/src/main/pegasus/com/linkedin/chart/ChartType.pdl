namespace com.linkedin.chart

/**
 * The various types of charts
 */
enum ChartType {

  /**
   * Chart showing a Bar chart
   */
  BAR

  /**
   * Chart showing a Pie chart
   */
  PIE

  /**
   * Chart showing a Scatter plot
   */
  SCATTER

  /**
   * Chart showing a table
   */
  TABLE

  /**
   * Chart showing Markdown formatted text
   */
  TEXT

  LINE

  AREA

  HISTOGRAM

  BOX_PLOT
}
